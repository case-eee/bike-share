<form action="/stations/<%= @station.id %>" method="POST">
<input type="hidden" name="_method" value="PUT">
<h3>Name <input type='text' name='station[name]' value="<%= @station.name %>"></h3>
<h3>Dock Count
<input type='text' name='station[dock_count]' value="<%= @station.dock_count %>"></h3>
<h3>City
<input type='text' name='station[city]' value="<%= @station.city %>"></h3>
<h3>Installation Date
<input type='text' name='station[installation_date]' value="<%= @station.installation_date %>"></h3>
<input type='submit' value="Submit">
</form>
