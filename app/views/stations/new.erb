


<form class="form-horizontal" action = "/stations" method = "post">
  <div class="form-group">
    <label for="inputEmail3" class="col-sm-2 control-label">Station Name</label>
    <div class="col-sm-10">
      <input type="Station Name" name="stations[name]" class="form-control" id="stations" placeholder="South Market St">
    </div>

    <div class="form-group">
      <label for="inputEmail3" class="col-sm-2 control-label">Dock Count</label>
      <div class="col-sm-10">
        <input type="Dock Count" name="stations[dock_count]" class="form-control" id="dock_count" placeholder="15">
      </div>

      <div class="form-group">
        <label for="inputEmail3" class="col-sm-2 control-label">City</label>
        <div class="col-sm-10">
          <input type="City" name="stations[city_id]" class="form-control" id="city" placeholder="San Francisco">
        </div>

        <div class="form-group">
          <label for="inputEmail3" class="col-sm-2 control-label">City</label>
          <div class="col-sm-10">
            <input type="Installation Date" name="stations[installation_date]" class="form-control" id="city" placeholder="2011-11-11">
          </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" class="btn btn-default" value = "Submit"></button>

    </div>
  </div>
</form>
